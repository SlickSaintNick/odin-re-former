<%= form_with model: @user do |form| %>
  <%= form.label :username, "Your username..." %></br>
  <%= form.text_field :username, value: "newbie123" %></br>
  <%= form.label :email %></br>
  <%= form.text_field :email, placeholder: "signup@email.com" %></br>
  <%= form.label :password, raw('Make a <a href="https://www.cyber.gov.au/protect-yourself/securing-your-accounts/passphrases/creating-strong-passphrases" target="_blank">Password</a>.') %></br>
  <%= form.text_field :password, value: "secret-passphrase-fools-every-attacker" %></br></br>
  <%= form.submit "Create" %>
<% end %>




